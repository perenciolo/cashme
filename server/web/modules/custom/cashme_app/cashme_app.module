<?php

/**
 * @file
 * Contains cashme_app.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function cashme_app_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the cashme_app module.
    case 'help.page.cashme_app':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('CashMe APP primary module') . '</p>';
      return $output;

    default:
  }
}
