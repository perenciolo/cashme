{#
/**
 * @file spent.html.twig
 * Default theme implementation to present Spent data.
 *
 * This template is used when viewing Spent pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_spent()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('spent') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
